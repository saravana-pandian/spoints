<odoo>

    <template id="index">
        <t t-call="website.layout">
            <t t-set="title">Shorepoint</t>
            <div class="oe_structure">
                <div class="container">
                    <t t-foreach="employees" t-as="employee">
                        <p><t t-esc="employee.id"/> <t t-esc="employee.name"/></p>
                    </t>
                </div>
            </div>
        </t>
    </template>

</odoo>
